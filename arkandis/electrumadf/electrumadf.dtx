%   &		&	ElectrumADFExp-Regular	&	yesr8a\\
%   &		&	ElectrumADFExp-Oblique	&	yesro8a\\
%   &		&	ElectrumADFExp-SemiBold		&	yess8a\\						
%   & 	&	ElectrumADFExp-SemiBoldOblique	&	yesso8a\\
%   &		&	ElectrumADFExp-Bold		&	yesb8a\\
%   &		&	ElectrumADFExp-BoldOblique	&	yesbo8a\\
%   \bottomrule
% \end{tabularx}
