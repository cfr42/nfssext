% For example, the following code sets the subset for \meta{family 1} to \texttt{1} and that for \meta{family 2} to \texttt{3}.
% \iffalse
%<*verb>
% \fi
\begin{verbatim}
fnt.subsetdefns.<family 1> = "1"
fnt.subsetdefns.<family 2> = "3"
\end{verbatim}
% \iffalse
%</verb>
% \fi
